{
  "presets": ["next/babel"],
  "plugins": [
    [
      "styled-components",
      {
        "displayName": true,
        "ssr": true
      }
    ],
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "utils": "./utils",
          "components": "./components",
          "constants": "./constants",
          "public": "./public"
        },
        "extensions": [".js", ".jsx", ".tsx"]
      }
    ],
    ["@babel/plugin-proposal-optional-chaining"],
    ["@babel/plugin-proposal-nullish-coalescing-operator"]
  ]
}
