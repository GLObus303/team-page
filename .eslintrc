{
  "parser": "@typescript-eslint/parser",
  "extends": ["@uplus/eslint-config-typescript"],
  "plugins": ["react", "import", "react-hooks"],
  "rules": {
    "@typescript-eslint/no-var-requires": 0,
    "react/prop-types": 0,
    "import/no-unresolved": 0,
    "react/display-name": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
